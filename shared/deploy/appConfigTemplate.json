{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "configurationStores_inferno_app_configuration_name": {
      "defaultValue": "meatgeek-app-configuration",
      "type": "String"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.AppConfiguration/configurationStores",
      "apiVersion": "2020-06-01",
      "name": "[parameters('configurationStores_inferno_app_configuration_name')]",
      "location": "westus2",
      "sku": {
        "name": "standard"
      },
      "properties": {
        "encryption": {}
      }
    }
  ]
}
